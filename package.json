{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc -w",
    "start": "nodemon --exitcrash -L src/server.ts",
    "lint": "eslint --ext .ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/common/ormconfig.ts",
    "mig:gen": "ts-node ./node_modules/typeorm/cli -f ./src/common/ormconfig.ts \"migration:generate\" \"-n\"",
    "mig:up": "ts-node ./node_modules/typeorm/cli -f ./src/common/ormconfig.ts \"migration:run\"",
    "mig:down": "ts-node ./node_modules/typeorm/cli -f ./src/common/ormconfig.ts \"migration:revert\""
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/swagger": "^5.2.0",
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "fastify": "^3.24.1",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.13.0",
    "jsonwebtoken": "^8.5.1",
    "openapi-validator-middleware": "^3.2.4",
    "pg": "^8.7.1",
    "pino": "^7.6.0",
    "pino-pretty": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.12",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "8.2.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "prettier": "^2.5.1",
    "supertest": "6.1.6",
    "ts-node-dev": "^1.1.8",
    "tsdoc": "^0.0.4",
    "typescript": "^4.5.2"
  }
}
